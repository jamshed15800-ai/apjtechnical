{
  "hosting": {
    "public": "frontend/build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/verify/**",
        "function": "verify"
      },
      {
        "source": "/api/**",
        "function": "createCert"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}